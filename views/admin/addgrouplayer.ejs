        <!-- ============================================================== -->
        <!-- ============================================================== -->
        <!-- wrapper  -->
        <!-- ============================================================== -->
        <div class="dashboard-wrapper">
            <div class="container-fluid dashboard-content">
                <!-- ============================================================== -->
                <!-- pageheader -->
                <!-- ============================================================== -->
                <div class="row">
                    <div class="col-xl-12 col-lg-12 col-md-12 col-sm-12 col-12">
                        <div class="page-header">
                            <h2 class="pageheader-title"><%= title %></h2>

                            <div class="page-breadcrumb">
                                <nav aria-label="breadcrumb">
                                    <ol class="breadcrumb">
                                        <li class="breadcrumb-item"><a href="#" class="breadcrumb-link">Dashboard</a>
                                        </li>
                                        <li class="breadcrumb-item"><a href="#" class="breadcrumb-link">UI Elements</a>
                                        </li>
                                        <li class="breadcrumb-item active" aria-current="page">Accordions</li>
                                    </ol>
                                </nav>
                            </div>
                        </div>
                    </div>
                </div>
                <!-- ============================================================== -->
                <!-- end pageheader -->
                <!-- ============================================================== -->
                <!-- ============================================================== -->
                <!-- accrodions -->
                <!-- ============================================================== -->
                <div id="sukses-alert" class="alert alert-primary" role="alert">
                    Success
                </div>
                <div class="row">

                    <!-- ============================================================== -->
                    <!-- accrodions style one -->
                    <!-- ============================================================== -->
                    <div class="col-xl-6 col-lg-6 col-md-12 col-sm-12 col-12">
                        <div class="section-block">
                            <h5 class="section-title">Tambah Group</h5>
                            <p>Grouping data GeoJSON berdasarkan jenis.</p>
                        </div>

                        <div class="card">
                            <div class="card-body">
                                <form action="/action/admin/addgrouplayer" method="POST">
                                    <div class="form-group">
                                        <label for="inputText3" class="col-form-label">Nama Group</label>
                                        <input id="inputText3" type="text" name="nama_group" class="form-control"
                                            placeholder="Misl. Tutupan Lahan">
                                    </div>
                                    <div class="form-group">
                                        <label for="exampleFormControlTextarea1">Deskripsi</label>
                                        <textarea class="form-control" name="desc_group"
                                            id="exampleFormControlTextarea1" rows="3"></textarea>
                                    </div>
                                    <input type="submit" class="btn btn-success">
                                </form>
                            </div>
                        </div>
                    </div>
                    <!-- ============================================================== -->
                    <!-- end accrodions style one -->
                    <!-- ============================================================== -->
                    <!-- ============================================================== -->
                    <!-- accrodions style two -->
                    <!-- ============================================================== -->
                    <div class="col-xl-6 col-lg-6 col-md-12 col-sm-12 col-12">
                        <div class="section-block">
                            <h5 class="section-title">List Grouping Layer</h5>
                            <p>List data GeoJSON berdasarkan group.</p>
                        </div>
                        <div class="accrodion-regular">
                            <div id="accordion">
                                <%
                                if(group_layer.length!=0){
                                var i=1;
                                group_layer.forEach(function(group){
                                %>
                                <div class="card">
                                    <div class="card-header" id="headingThree">
                                        <h5 class="mb-0">
                                            <div style="text-align:left" class="btn btn-link collapsed"
                                                data-toggle="collapse" data-target="#collapseThree"
                                                aria-expanded="false" aria-controls="collapseThree">

                                                <%=group.nama_group %>   
                                                
                                            </div>
                                            <span class="btn-right">
                                                <a href="" class="btn btn-rounded btn-info">List Data</a>
                                                <a href="/action/admin/editgrouplayer/<%=group.id_group %>" class="btn btn-rounded btn-brand">Edit</a>
                                                <a href="/action/admin/deletegroup/<%=group.id_group %>" class="btn btn-rounded btn-danger">Delete</a>
                                            </span>
                                        </h5>
                                    </div>
                                </div>
                                <%  i++; }) %>
                                <% } else { %>
                                <div class="card">
                                    <div class="card-header" id="headingThree">
                                        <h5 class="mb-0">
                                            <button class="btn btn-link collapsed" data-toggle="collapse"
                                                data-target="#collapseThree" aria-expanded="false"
                                                aria-controls="collapseThree">
                                                No Group Added
                                            </button>
                                        </h5>
                                    </div>
                                </div>
                                <% } %>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
